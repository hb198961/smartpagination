<?xml version="1.0" encoding="ISO-8859-1"?>
<ivysettings>
	<settings defaultResolver="defaultChain" checkUpToDate="true"
		defaultConflictManager="latest-revision" />
	<resolvers>
		<chain name="defaultChain">
			<url name="com.springsource.repository.bundles.release">
				<ivy
					pattern="http://repository.springsource.com/ivy/bundles/release/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
				<artifact
					pattern="http://repository.springsource.com/ivy/bundles/release/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
			</url>
			<url name="com.springsource.repository.bundles.external">
				<ivy
					pattern="http://repository.springsource.com/ivy/bundles/external/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
				<artifact
					pattern="http://repository.springsource.com/ivy/bundles/external/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
			</url>
			<ibiblio name="ibiblio" m2compatible="true" />
		</chain>
		<filesystem name="local" checkmodified="true">
			<ivy pattern="${repository.dir}/[organisation]/[module]/ivys/ivy-[revision].xml"/>
  			<artifact pattern="${repository.dir}/[organisation]/[module]/[type]s/[artifact]-[revision].[ext]"/>
		</filesystem>
	</resolvers>
</ivysettings>